# This is Git's per-user configuration file.
[user]
	name = Brandon Moon
	email = brandon.moon@liveauctioneers.com
[pull]
	rebase = true ; Rebase instead of merge when pulling
[push]
	default = current
[remote]
	pushDefault = origin
[help]
	autocorrect = 1 ; Autocorrect mistyped commands
[core]
	excludesfile = /Users/bmoon/.gitignore_global
[alias]
	co = checkout
	br = branch
[credential]
	helper = /usr/local/share/gcm-core/git-credential-manager
[credential "https://dev.azure.com"]
	useHttpPath = true
